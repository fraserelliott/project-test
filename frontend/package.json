{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "setup": "node ../scripts/setupProject.js",
    "dev-mock": "concurrently \"cross-env VITE_API_URL=http://localhost:8000 npm run dev\" \"npm run mock-server\"",
    "dev-backend": "concurrently \"cd ../backend && npm run dev\" \"cross-env VITE_API_URL=http://localhost:3001 npm run dev\"",
    "mock-server": "node ../scripts/runMock.js",
    "backend": "cd ../backend && npm run dev"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
